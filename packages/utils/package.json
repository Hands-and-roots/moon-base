{
  "name": "@hands-and-roots/utils",
  "private": true,
  "version": "0.0.1",
  "description": "A Node.js package.",
  "keywords": [
    "node",
    "lib"
  ],
  "author": "Mohammad Hefazi <smohe1991@gmail.com>",
  "license": "MIT",
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "engines": {
    "node": ">=18.12.0",
    "yarn": ">=1.22.22"
  },
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  }
}
